<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>{{ ssh_agent_launchd_service }}</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>{{ ssh_agent_bin_path }} -D -a {{ ssh_agent_socket_path }}</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
